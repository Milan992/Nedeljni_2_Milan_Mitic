<Window x:Class="WpfClinic.Views.Master"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfClinic.Views"
        mc:Ignorable="d"
        Title="Master" Height="450" Width="800">
    <Grid>
        <Button Content="Add administrator"
                HorizontalAlignment="Left"
                Margin="10,10,0,0" VerticalAlignment="Top" 
                Width="120"
                Command="{Binding AddAdmin, UpdateSourceTrigger=PropertyChanged}"
                Height="33"/>
        <Button Content="Add manager"
                HorizontalAlignment="Left"
                Margin="10,50,0,0" VerticalAlignment="Top" 
                Width="120"
                Command="{Binding AddManager, UpdateSourceTrigger=PropertyChanged}"
                Height="33"/>
    </Grid>
</Window>
